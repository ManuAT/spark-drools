package com.ts.blog.rules

import org.slf4j.Logger
import com.ts.blog.rules.Blog

global Logger LOGGER

rule cat_top
     salience 100
     agenda-group "java_ag"
     no-loop true
     activation-group "java_ac"
     dialect "java"
when
    $blog:Blog( likes > 100 )
then
    modify( $blog )(setCategory(""))